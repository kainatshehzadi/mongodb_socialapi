{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
      
    }
  ],
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "app/main.py",
      "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ]
}